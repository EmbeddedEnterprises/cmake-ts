{
  "name": "cmake-ts",
  "version": "0.1.13",
  "description": "cmake-js rewrite in typescript to support advanced build configurations",
  "main": "build/lib.js",
  "bin": "build/main.js",
  "scripts": {
    "deps": "npm-check -p",
    "devdeps": "npm-check -ds",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc -w",
    "build": "shx rm -rf build && tsc"
  },
  "files": [
    "build/**/*.js",
    "build/**/*.js.map",
    "build/**/*.d.ts",
    "build/**/*.d.ts.map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmbeddedEnterprises/cmake-ts.git"
  },
  "keywords": [
    "cmake",
    "nan",
    "node",
    "native",
    "addon",
    "build",
    "cmake-js"
  ],
  "author": "Martin Koppehel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmbeddedEnterprises/cmake-ts/issues"
  },
  "homepage": "https://github.com/EmbeddedEnterprises/cmake-ts#readme",
  "devDependencies": {
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.10",
    "@types/npmlog": "^4.1.1",
    "@types/request": "^2.48.1",
    "@types/semver": "^5.5.0",
    "@types/tar": "^4.0.0",
    "@types/url-join": "^4.0.0",
    "@types/which": "^2.0.0",
    "npm-check": "^5.9.0",
    "shx": "^0.3.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/lodash": "^4.14.120",
    "@types/unzipper": "^0.9.1",
    "find": "^0.3.0",
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.11",
    "memory-stream": "0.0.3",
    "npmlog": "^4.1.2",
    "queueable": "^4.1.2",
    "request": "^2.88.0",
    "semver": "^5.6.0",
    "splitargs": "0.0.7",
    "tar": "^4.4.8",
    "unzipper": "^0.9.10",
    "url-join": "^4.0.0",
    "which": "^2.0.2",
    "zlib": "^1.0.5"
  }
}
